{
  "name": "shinkansen-gears",
  "version": "1.1.54",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./src/index.cjs",
  "type": "module",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "build": "cross-env DEBUG=shinkansen-gears npm run gulp -- build",
    "build:watch": "cross-env DEBUG=shinkansen-gears npm run gulp -- build:watch",
    "transform": "cross-env DEBUG=shinkansen-gears npm run gulp -- transform",
    "transform:watch": "cross-env DEBUG=shinkansen-gears npm run gulp -- transform:watch",
    "pregulp": "npm run nvm",
    "gulp": "gulp --color",
    "prelint": "npm run nvm",
    "lint": "cross-env NODE_ENV=production eslint . .storybook --ext .mjs,.cjs,.jsx,.mts,.cts",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run nvm",
    "test": "cross-env NODE_ENV=test jest",
    "nvm": "bash nvm.sh",
    "prestorybook": "npm run build && npm run transform",
    "storybook": "storybook dev -p 6008",
    "prepare": "husky install",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.22.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@modernpoacher/design-system": "1.0.52",
    "@modernpoacher/hooks": "^1.0.421",
    "@sequencemedia/gulp": "^1.1.31",
    "@sequencemedia/gulp-clean-css": "^1.0.13",
    "@sequencemedia/gulp-css-purge": "^1.1.6",
    "@sequencemedia/gulp-debug": "^1.0.17",
    "@sequencemedia/gulp-postcss": "^1.0.13",
    "@sequencemedia/gulp-sass": "^1.0.12",
    "@sequencemedia/gulp-sourcemaps": "^1.0.11",
    "@storybook/addon-actions": "^7.0.24",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-webpack5": "^7.0.24",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "autoprefixer": "^10.4.14",
    "babel-plugin-module-resolver": "^5.0.0",
    "core-js": "^3.31.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del": "^7.0.0",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "35.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-react": "^7.32.2",
    "glob-all": "^3.3.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "postcss": "^8.4.24",
    "postcss-easy-import": "^4.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.63.6",
    "shinkansen-rails": "1.0.77",
    "storybook": "^7.0.24",
    "vinyl-paths": "^5.0.0"
  },
  "imports": {
    "#gears-reverse": "./src/gears/reverse/index.cjs",
    "#gears": "./src/gears/index.cjs",
    "#build/gulp/build/*": "./build/gulp/build/*.mjs",
    "#build/gulp/build": "./build/gulp/build/index.mjs",
    "#build/gulp/transform": "./build/gulp/transform/index.mjs",
    "#build/gulp/*": "./build/gulp/*.mjs",
    "#build/paths": "./build/paths.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./gears/forward": "./src/gears/forward/index.cjs",
    "./gears/reverse": "./src/gears/reverse/index.cjs",
    "./gears": "./src/gears/index.cjs"
  }
}
