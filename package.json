{
  "name": "shinkansen-gears",
  "version": "1.1.259",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./src/index.cjs",
  "type": "module",
  "types": "./src/index.d.mts",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "http://modernpoacher.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "build": "cross-env DEBUG=shinkansen-gears npm run gulp -- build",
    "build:watch": "cross-env DEBUG=shinkansen-gears npm run gulp -- build:watch",
    "transform": "cross-env DEBUG=shinkansen-gears npm run gulp -- transform",
    "transform:watch": "cross-env DEBUG=shinkansen-gears npm run gulp -- transform:watch",
    "pregulp": "npm run nvm",
    "gulp": "gulp --color",
    "prelint": "npm run nvm",
    "lint": "eslint . .storybook --ext .mjs,.cjs,.jsx,.mts,.cts",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run nvm",
    "test": "cross-env NODE_ENV=test jest",
    "nvm": "bash nvm.sh",
    "prestorybook": "npm run build && npm run transform",
    "storybook": "storybook dev -p 6008",
    "prepare": "husky",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@modernpoacher/design-system": "1.0.137",
    "@modernpoacher/hooks": "^1.0.467",
    "@sequencemedia/gulp": "^1.1.168",
    "@sequencemedia/gulp-clean-css": "^1.0.87",
    "@sequencemedia/gulp-css-purge": "^1.1.85",
    "@sequencemedia/gulp-debug": "^1.0.90",
    "@sequencemedia/gulp-postcss": "^1.0.98",
    "@sequencemedia/gulp-sass": "^1.0.86",
    "@sequencemedia/gulp-sourcemaps": "^1.0.89",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@types/react": "^18.2.61",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "babel-plugin-module-resolver": "^5.0.0",
    "core-js": "^3.36.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.5",
    "del": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-react": "^7.33.2",
    "glob-all": "^3.3.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "postcss-easy-import": "^4.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.9",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.2",
    "react-router-dom": "^6.22.2",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.71.1",
    "shinkansen-rails": "^1.0.143",
    "storybook": "^7.6.17",
    "vinyl-paths": "^5.0.0"
  },
  "imports": {
    "#build/gulp/build/*": "./build/gulp/build/*.mjs",
    "#build/gulp/build": "./build/gulp/build/index.mjs",
    "#build/gulp/transform": "./build/gulp/transform/index.mjs",
    "#build/gulp/*": "./build/gulp/*.mjs",
    "#build/paths": "./build/paths.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./gears/forward": "./src/gears/forward/index.cjs",
    "./gears/reverse": "./src/gears/reverse/index.cjs",
    "./gears": "./src/gears/index.cjs"
  }
