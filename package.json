{
  "name": "shinkansen-gears",
  "version": "1.2.9",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./src/index.cjs",
  "type": "module",
  "types": "./index.d.mts",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "https://modernpoacher.com"
  },
  "license": "ISC",
  "engines": {
    "node": ">=22.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "build": "cross-env DEBUG=shinkansen-gears* webpack",
    "build-storybook": "storybook build",
    "build:watch": "cross-env DEBUG=shinkansen-gears* webpack --watch",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "nvm": "bash nvm.sh",
    "prelint": "npm run nvm",
    "prepare": "husky",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prestorybook": "npm run build && npm run transform",
    "pretest": "npm run nvm",
    "storybook": "storybook dev -p 6008",
    "test": "cross-env NODE_ENV=test jest",
    "transform": "cross-env DEBUG=shinkansen-gears* ./transform.sh"
  },
  "dependencies": {
    "debug": "^4.4.0",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@modernpoacher/design-system": "1.1.30",
    "@sequencemedia/eslint-config-standard": "^0.2.35",
    "@sequencemedia/eslint-config-typescript": "^0.1.50",
    "@storybook/addon-actions": "7.6.20",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/addon-links": "7.6.20",
