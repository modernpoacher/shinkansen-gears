{
  "name": "shinkansen-gears",
  "version": "1.0.26",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env DEBUG=shinkansen-gears npm run babel -- --ignore \"**/__tests__/**\" && npm run gulp",
    "build:watch": "npm run gulp -- build:watch",
    "watch": "cross-env DEBUG=shinkansen-gears concurrently -r \"npm run babel:watch\" \"npm run build:watch\"",
    "clean": "rimraf lib",
    "transform": "npm run gulp -- transform",
    "transform:watch": "npm run gulp -- transform:watch",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "gulp": "bash gulp.sh --color",
    "lint": "eslint . .storybook",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --verbose --run-in-band",
    "storybook": "npm run build && npm run transform && start-storybook -p 6008",
    "build-storybook": "build-storybook",
    "pre-commit": "npm run lint && npm test && npm run build && npm run transform && git add lib .storybook"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "immutable": "3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@modernpoacher/design-system": "0.0.610",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cheerio": "1.0.0-rc.6",
    "concurrently": "^7.2.2",
    "core-js": "^3.23.4",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "del": "^6.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "glob-all": "^3.3.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "postcss": "^8.4.14",
    "postcss-easy-import": "^4.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.4",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.14.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "shinkansen-rails": "1.0.18",
    "vinyl-paths": "3.0.1"
  }
}
