{
  "name": "shinkansen-gears",
  "version": "1.0.19",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=development concurrently -r \"npm run babel -- --ignore \"**/__tests__/**\"\" \"gulp build\"",
    "build:watch": "cross-env NODE_ENV=development concurrently -r \"npm run babel:watch\" \"gulp build:watch\"",
    "transform": "gulp transform",
    "transform:watch": "gulp transform:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --verbose",
    "storybook": "npm run build && npm run transform && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "npm run lint && npm test && npm run build && npm run transform && git add lib .storybook"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "immutable": "3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@modernpoacher/design-system": "0.0.67",
    "@storybook/addon-actions": "^6.0.5",
    "@storybook/addon-links": "^6.0.5",
    "@storybook/react": "^6.0.5",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "glob-all": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^9.0.0",
    "postcss-scss": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
