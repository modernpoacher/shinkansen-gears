{
  "name": "shinkansen-gears",
  "version": "1.0.28",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./index.mjs",
  "type": "module",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run nvm && cross-env DEBUG=shinkansen-gears npm run babel -- --ignore \"**/__tests__/**\" && npm run gulp",
    "build:watch": "npm run gulp -- build:watch",
    "watch": "npm run nvm && cross-env DEBUG=shinkansen-gears concurrently -r \"npm run babel:watch\" \"npm run build:watch\"",
    "clean": "rimraf lib",
    "transform": "npm run gulp -- transform",
    "transform:watch": "npm run gulp -- transform:watch",
    "babel": "babel src -d lib --out-file-extension .cjs",
    "babel:watch": "npm run babel -- -w",
    "gulp": "gulp --color",
    "lint": "cross-env NODE_ENV=production eslint . .storybook --ext .mjs --ext .cjs --ext .jsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run nvm && cross-env NODE_ENV=test jest --collect-coverage false --verbose --run-in-band",
    "nvm": "bash nvm.sh",
    "storybook": "npm run build && npm run transform && start-storybook -p 6008"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "immutable": "3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-syntax-jsx": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@modernpoacher/design-system": "0.0.945",
    "@modernpoacher/hooks": "^1.0.391",
    "@sequencemedia/gulp": "^1.0.5",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/react": "6.5.12",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "cheerio": "1.0.0-rc.6",
    "concurrently": "^8.0.1",
    "core-js": "^3.30.1",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "glob-all": "^3.3.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "postcss-easy-import": "^4.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.6",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.14.0",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "shinkansen-rails": "1.0.18",
    "vinyl-paths": "^5.0.0"
  },
  "imports": {
    "#gears/forward": "./src/gears/forward/index.cjs",
    "#gears/reverse": "./src/gears/forward/index.cjs",
    "#gears": "./src/gears/index.cjs",
    "#build/gulp/build": "./build/gulp/build/index.mjs",
    "#build/gulp/build/css-from-sass": "./build/gulp/build/css-from-sass.mjs",
    "#build/gulp/build/css": "./build/gulp/build/css.mjs",
    "#build/gulp/build/fonts": "./build/gulp/build/fonts.mjs",
    "#build/gulp/build/icons": "./build/gulp/build/icons.mjs",
    "#build/gulp/paths": "./build/gulp/paths.mjs",
    "#build/gulp/handle-watch-error": "./build/gulp/handle-watch-error.mjs",
    "#build/gulp/transform": "./build/gulp/transform/index.mjs",
    "#build/paths": "./build/paths.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./gears/forward": "./src/gears/forward/index.cjs",
    "./gears/reverse": "./src/gears/reverse/index.cjs",
    "./gears": "./src/gears/index.cjs"
  }
}
