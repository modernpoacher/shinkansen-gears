{
  "name": "shinkansen-gears",
  "version": "1.1.0",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./src/index.cjs",
  "type": "module",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "build": "cross-env DEBUG=shinkansen-gears npm run gulp -- build",
    "build:watch": "cross-env DEBUG=shinkansen-gears npm run gulp -- build:watch",
    "transform": "cross-env DEBUG=shinkansen-gears npm run gulp -- transform",
    "transform:watch": "cross-env DEBUG=shinkansen-gears npm run gulp -- transform:watch",
    "pregulp": "npm run nvm",
    "gulp": "gulp --color",
    "prelint": "npm run nvm",
    "lint": "cross-env NODE_ENV=production eslint . .storybook --ext .mjs --ext .cjs --ext .jsx --ext .mts --ext .cts",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run nvm",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --verbose --run-in-band",
    "nvm": "bash nvm.sh",
    "storybook": "npm run build && npm run transform && start-storybook -p 6008",
    "prepare": "husky install"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-syntax-jsx": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@modernpoacher/design-system": "1.0.9",
    "@modernpoacher/hooks": "^1.0.399",
    "@sequencemedia/gulp": "^1.0.7",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "7.0.10",
    "@storybook/addon-links": "6.5.12",
    "@storybook/react": "6.5.12",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "babel-plugin-module-resolver": "^5.0.0",
    "core-js": "^3.30.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del": "^7.0.0",
    "eslint": "^8.40.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "glob-all": "^3.3.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "postcss-easy-import": "^4.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "shinkansen-rails": "1.0.41",
    "vinyl-paths": "^5.0.0"
  },
  "imports": {
    "#gears-reverse": "./src/gears/reverse/index.cjs",
    "#gears": "./src/gears/index.cjs",
    "#build/gulp/build/*": "./build/gulp/build/*.mjs",
    "#build/gulp/build": "./build/gulp/build/index.mjs",
    "#build/gulp/transform": "./build/gulp/transform/index.mjs",
    "#build/gulp/*": "./build/gulp/*.mjs",
    "#build/paths": "./build/paths.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./gears/forward": "./src/gears/forward/index.cjs",
    "./gears/reverse": "./src/gears/reverse/index.cjs",
    "./gears": "./src/gears/index.cjs"
  }
}
