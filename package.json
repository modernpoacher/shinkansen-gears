{
  "name": "shinkansen-gears",
  "version": "1.0.22",
  "description": "Shinkansen Gears",
  "keywords": [
    "Shinkansen",
    "Gears",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-gears.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env DEBUG=shinkansen:gears npm run babel -- --ignore \"**/__tests__/**\" && gulp build",
    "watch": "cross-env DEBUG=shinkansen:gears concurrently -r \"npm run babel:watch\" \"npm run gulp -- build:watch\"",
    "clean": "rimraf lib",
    "transform": "npm run gulp -- transform",
    "transform:watch": "npm run gulp -- transform:watch",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "gulp": "gulp --color",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test jest --collect-coverage false --verbose --run-in-band",
    "storybook": "npm run build && npm run transform && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "npm run lint && npm test && npm run build && npm run transform && git add lib .storybook"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "immutable": "3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@modernpoacher/design-system": "0.0.167",
    "autoprefixer": "^10.0.4",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^5.0.0",
    "glob-all": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "postcss": "^8.1.14",
    "postcss-easy-import": "^3.0.0",
    "postcss-map": "^0.11.0",
    "postcss-normalize": "^9.0.0",
    "postcss-scss": "^3.0.4",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.14.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "shinkansen-rails": "1.0.17",
    "vinyl-paths": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
