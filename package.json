    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=development concurrently -r \"npm run babel\" \"gulp build\"",
    "build:watch": "cross-env NODE_ENV=development concurrently -r \"npm run babel:watch\" \"gulp build:watch\"",
    "transform": "gulp transform",
    "transform:watch": "gulp transform:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src build stories *.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test jest --verbose false --collect-coverage false",
    "storybook": "npm run build && npm run transform && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "npm run lint && npm test && npm run build && npm run transform && git add lib .storybook"
  },
