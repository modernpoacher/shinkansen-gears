    "@types/babel__register": "^7.17.3",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/react": "18.3.13",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "autoprefixer": "^10.4.20",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.40.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "perfectionist": "^2.4.0",
    "postcss": "^8.5.3",
    "postcss-discard-comments": "^7.0.3",
    "postcss-discard-empty": "^7.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-map": "^0.11.0",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "^7.2.0",
    "react-test-renderer": "18.3.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "shinkansen-rails": "1.0.293",
    "storybook": "7.6.20",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "shinkansen-rails": "1.0.x"
  },
  "imports": {
    "#build/paths": "./build/paths.mjs",
    "#gears": "./src/index.mjs",
    "#gears/gears": "./src/gears/index.jsx",
    "#gears/gears/*": "./src/gears/*/index.jsx",
    "#gears/gears/forward": "./src/gears/forward/index.jsx",
    "#gears/gears/reverse": "./src/gears/reverse/index.jsx"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./gears": "./src/gears/index.cjs",
    "./gears/forward": "./src/gears/forward/index.cjs",
    "./gears/reverse": "./src/gears/reverse/index.cjs"
  }
