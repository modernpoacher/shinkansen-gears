{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            "last 4 versions",
            "safari >= 9",
            "ios >= 8",
            "ie >= 9",
            "> 2%"
          ]
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/syntax-jsx",
    "@babel/proposal-export-default-from",
    "@babel/proposal-export-namespace-from",
    [
      "@babel/proposal-class-properties",
      {
        "loose": false
      }
    ],
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {
          "shinkansen-gears/forward": "./gears/forward/index.jsx",
          "shinkansen-gears/reverse": "./gears/reverse/index.jsx",
          "shinkansen-gears": "./gears/index.jsx",
          "build": "../build",
          "stories": "../stories"
        }
      }
    ]
  ]
}
